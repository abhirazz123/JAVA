public class MyClass {
    private int number = 10;
    
    public int getNumber() {
        return number;
    }
    
    public void display() {
        System.out.println(number);
    }
}

public class Main {
    public static void main(String[] args) {
        MyClass obj = new MyClass();
        System.out.println(obj.getNumber());
    }
}
